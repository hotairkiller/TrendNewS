---
import Hamburger from "./Hamburger.astro"
import ThemeIcon from './ThemeIcon.astro';
import Navigation from "./Navigation.astro"

const address = {
  "a":'home',
  "b":"about",
  "c":"blog"

}


const { a, b, c } = address;

---

<header>
    <Hamburger / >
    <ThemeIcon />
    <Navigation webPage="/" address1={a}/>
    <Navigation webPage="about" address1 ={b} />
    <Navigation webPage="blog"  address1 = {c}/>
    
</header>

<style>
   header{
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>